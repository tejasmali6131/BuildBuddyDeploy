{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/models/(.*\\.glb)",
      "headers": {
        "content-type": "application/octet-stream",
        "cache-control": "s-maxage=31536000,immutable"
      },
      "dest": "/models/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}